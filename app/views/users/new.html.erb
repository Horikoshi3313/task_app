<h1>スケジュール管理アプリ</h1>

<h2>スケジュール新規作成</h2>
 
    <%= form_for @user do |f| %>
      <table>
        <tr>
          <th><%= f.label :title ,"タイトル" %></th>
          <td><%= f.text_field :title %></td>
        </tr>
 
        <tr>
          <th><%= f.label :start ,"開始日" %></th>
          <td><%= f.datetime_field :start %><td>
        </tr>
 
        <tr>
          <th><%= f.label :end ,"終了日" %></th>
          <td><%= f.datetime_field :end %><td>
        </tr>
        
        <tr>
          <th><%= f.label :member ,"メンバー" %></th>
      <td><%= f.text_field :member %></td>
        </tr>

 
        <tr>
          <th><%= f.label :text ,"備考" %></th>
          <td><%= f.text_area :text %></td>
        </tr>
      </table>
 
      <div>
        <ul>
          <li><%= f.submit "スケジュールを新規登録"%></li>
          <li><%= link_to "スケジュール情報一覧に戻る", :users %></li>
        </ul>
      </div>
<% end %>